{
  "cSpell.words": [
    "Expressable",
    "declarapi"
  ],
  "standard.engine": "standardx",
  "standard.usePackageJson": true,
  "jest.showCoverageOnLoad": false,
  "standard.autoFixOnSave": true,
  "eslint.enable": false,
  "files.exclude": {
    "**/node_modules": true,
    "integrationTests/dist": true,
    "integrationTests/**/*.js": false,
    "integrationTests/coverage": true,
    "integrationTests/temp/*.ts": true,
    "*.js":true,
    "**/*.js.map": true,
    "**/*.d.ts": true,
    "**/cjs": true,
    "testout": true,
    "testOut": true,
    "dist": true,
    "lib": true,
    "node_modules": true,
    ".pnp": true,
    "**/.pnp.js": true,
    "test": true,
    "**/*.tgz": true,
    "coverage": true,
    "build": true,
    "**/.env": true,
    "**/.DS_Store": true,
    "**/.env.local": true,
    "**/.env.development.local": true,
    "**/.env.test.local": true,
    "**/.env.production.local": true,
    "**/npm-debug.log*": true,
    "**/yarn-debug.log*": true,
    "**/yarn-error.log*": true,
    "integrationTests/setup/kvWorker/**/coverage": true,
    "integrationTests/setup/elastic/**/coverage": true,
    "integrationTests/temp/**/*.ts": true,
    "integrationTests/setup/kvMemory/**/coverage": true
  }
}
