{
  "globals": {
    "ts-jest": {
      "tsConfig": "<rootDir>/../../tsconfig.json"
    }
  },
  "setupFiles": [
    "./env.js"
  ],
  "globalSetup": "./global.js",
  "globalTeardown": "./teardown.js",
  "setupFilesAfterEnv": ["./setup.js"],
  "roots": [
    "<rootDir>../../src"
  ],
  "moduleNameMapper": {
    "yaschva$": "yaschva/cjs",
    "microtil$": "microtil/cjs",
    "declarapi-runtime/elastic$": "declarapi-runtime/cjs/elastic.js",
    "declarapi-runtime$": "declarapi-runtime/cjs"
  }  ,
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "collectCoverageFrom": [
    "**/*.{js,jsx,ts,tsx}",
    "!**/node_modules/**",
    "!./src/bin/**",
    "!**/vendor/**"
  ],
  "resetModules": true,
  "clearMocks": true,
  "testTimeout": 300000
}
